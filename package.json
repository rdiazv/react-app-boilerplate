{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "A react app boilerplate.",
  "main": "index.js",
  "repository": "git@github.com:rdiazv/react-app-boilerplate.git",
  "author": "Rodrigo Andrés Díaz Vergara <rdiazv89@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.js -p",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.js --hot",
    "profile": "NODE_ENV=production webpack --config webpack/webpack.config.js -p --profile --json > webpack.stats.json",
    "start": "node server.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "compression-webpack-plugin": "0.4.0",
    "css-loader": "0.28.4",
    "directory-named-webpack-plugin": "2.1.0",
    "dotenv-webpack": "1.5.0",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "express": "4.15.3",
    "express-static-gzip": "0.2.2",
    "extract-text-webpack-plugin": "2.1.0",
    "history": "^4.5.1",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "jest-directory-named-resolver": "jsonnull/jest-directory-named-resolver",
    "jest-enzyme": "3.2.0",
    "postcss-automath": "1.0.1",
    "postcss-color-function": "4.0.0",
    "postcss-comment": "1.0.3",
    "postcss-hexrgba": "0.2.1",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.5",
    "postcss-mixins": "6.0.0",
    "postcss-nested": "2.0.2",
    "postcss-simple-vars": "4.0.0",
    "react-test-renderer": "15.5.4",
    "style-loader": "0.18.1",
    "webpack": "2.6.1",
    "webpack-config": "7.0.0",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "deep-assign": "2.0.0",
    "promise-polyfill": "6.0.2",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router-dom": "4.1.1",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "testRegex": "-test.js$",
    "resolver": "jest-directory-named-resolver",
    "moduleDirectories": [
      "src",
      "tests",
      "node_modules"
    ],
    "collectCoverage": true,
    "coverageDirectory": "tests/coverage",
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  },
  "engines": {
    "node": "6.x"
  }
}
